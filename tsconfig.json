{
	"compilerOptions": {
    "outDir": "dist",
		"rootDir": "./src",
		"target": "es6",
		"module": "commonjs",
		"lib": ["es2015", "es2016", "es2017"],
		"declaration": true,
		"strict": true,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"moduleResolution": "node"
	}
}
